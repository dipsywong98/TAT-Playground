{
  "name": "typescript-apollo-typeorm-playground",
  "version": "1.0.0",
  "description": "just a playground",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "webpack --config webpack.prod.js",
    "both": "concurrently --kill-others-on-fail \"npm run watch\" \"npm run dev\"",
    "watch": "webpack --config webpack.dev.js",
    "dev": "node --inspect dist/index.js",
    "test": "jest --verbose",
    "lint": "tslint -c tslint.json -t code-frame lib/**/*.ts"
  },
  "author": ".zinc team",
  "license": "ISC",
  "dependencies": {
    "apollo-datasource": "^0.1.3",
    "apollo-datasource-rest": "^0.1.5",
    "apollo-server": "2.2.0-alpha.2",
    "apollo-server-testing": "2.2.0-alpha.2",
    "graphql": "^14.0.2",
    "graphql-schema-tools": "0.0.6",
    "mysql": "^2.17.1",
    "nodemon": "^1.18.4",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.18"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.2",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.6.2",
    "@types/webpack-env": "^1.14.0",
    "apollo-link": "^1.2.3",
    "apollo-link-http": "^1.5.5",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "jest": "^23.6.0",
    "nock": "^10.0.2",
    "node-fetch": "^2.2.1",
    "ts-jest": "^24.0.2",
    "ts-loader": "^6.0.4",
    "tsc-watch": "^2.2.1",
    "typescript": "^3.5.3",
    "webpack": "^4.35.3",
    "webpack-cli": "^3.3.6",
    "webpack-merge": "^4.2.1",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/__utils"
    ]
  }
}
